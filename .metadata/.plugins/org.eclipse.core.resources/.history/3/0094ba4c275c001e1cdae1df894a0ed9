/*
 * package com.indianbank.service;
 * 
 * import org.springframework.stereotype.Service;
 * 
 * import com.indianbank.entity.Transactions; import
 * com.indianbank.repository.TransactionRepository;
 * 
 * @Service public class TransactionService { private TransactionRepository
 * transactionRepository;
 * 
 * public TransactionService(TransactionRepository transactionRepository) {
 * super(); this.transactionRepository = transactionRepository; }
 * 
 * public Transactions saveTransaction(Transactions transaction) { return
 * transactionRepository.save(transaction); }
 * 
 * }
 */